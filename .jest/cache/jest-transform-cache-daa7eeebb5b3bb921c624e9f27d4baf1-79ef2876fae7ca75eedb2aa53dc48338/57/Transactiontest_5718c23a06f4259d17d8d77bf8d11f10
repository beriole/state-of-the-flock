_getJestObj().mock("../utils/fakeBudgetUtils", function () {
  return {
    categoriserDepense: jest.fn(function (depense) {
      return "Alimentation";
    }),
    genererStatistiques: jest.fn(function () {
      return {
        total: 50000,
        categories: {
          Alimentation: 20000,
          Transport: 30000
        }
      };
    }),
    recommandationsBudget: jest.fn(function () {
      return ["Réduire les sorties restaurant", "Optimiser les transports"];
    })
  };
});
