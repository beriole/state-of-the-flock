var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("react-native");
var _reactNative2 = require("@testing-library/react-native");
var _jsxRuntime = require("react/jsx-runtime");
describe("Module d'Épargne Planifiée", function () {
  var calculerProgression = function calculerProgression(objectif) {
    return Math.round(objectif.epargneActuelle / objectif.epargneCible * 100);
  };
  var suivreObjectifs = function suivreObjectifs() {
    return [{
      nom: "Vacances",
      epargneCible: 100000,
      epargneActuelle: 25000
    }, {
      nom: "Achat voiture",
      epargneCible: 500000,
      epargneActuelle: 100000
    }];
  };
  it("calcule correctement la progression d'un objectif", function () {
    var objectif = {
      nom: "Vacances",
      epargneCible: 100000,
      epargneActuelle: 25000
    };
    var progression = calculerProgression(objectif);
    expect(progression).toBe(25);
    console.log("✅ Progression de l'objectif calculée correctement");
  });
  it("suit correctement les objectifs d'épargne", function () {
    var objectifs = suivreObjectifs();
    expect(objectifs).toHaveLength(2);
    expect(objectifs[0].nom).toBe("Vacances");
    expect(objectifs[1].nom).toBe("Achat voiture");
    console.log("✅ Objectifs d'épargne suivis correctement");
  });
  it("simule l'affichage de l'avancement des objectifs", function () {
    var DummyComponent = function DummyComponent() {
      return (0, _jsxRuntime.jsxs)(_jsxRuntime.Fragment, {
        children: [(0, _jsxRuntime.jsx)(_reactNative.Text, {
          children: "Objectif: Vacances - Progression: 25%"
        }), (0, _jsxRuntime.jsx)(_reactNative.Text, {
          children: "Objectif: Achat voiture - Progression: 20%"
        })]
      });
    };
    var _render = (0, _reactNative2.render)((0, _jsxRuntime.jsx)(DummyComponent, {})),
      getByText = _render.getByText;
    expect(getByText("Objectif: Vacances - Progression: 25%")).toBeTruthy();
    expect(getByText("Objectif: Achat voiture - Progression: 20%")).toBeTruthy();
    console.log("✅ Composants d'avancement des objectifs affichés correctement");
  });
});
