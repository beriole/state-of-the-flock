var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("react-native");
var _reactNative2 = require("@testing-library/react-native");
var _jsxRuntime = require("react/jsx-runtime");
describe("Module de Planification de Projets Personnels", function () {
  var creerProjet = function creerProjet(nom, budgetTotal) {
    return {
      nom: nom,
      budgetTotal: budgetTotal,
      depenses: 0
    };
  };
  var allouerDepense = function allouerDepense(projet, montant) {
    projet.depenses += montant;
    return projet;
  };
  var calculerProgression = function calculerProgression(projet) {
    return Math.round(projet.depenses / projet.budgetTotal * 100);
  };
  it("crée correctement un projet personnel", function () {
    var projet = creerProjet("Déménagement", 150000);
    expect(projet.nom).toBe("Déménagement");
    expect(projet.budgetTotal).toBe(150000);
    expect(projet.depenses).toBe(0);
    console.log("✅ Projet personnel créé correctement");
  });
  it("alloue correctement un montant aux dépenses du projet", function () {
    var projet = creerProjet("Voyage", 200000);
    projet = allouerDepense(projet, 50000);
    expect(projet.depenses).toBe(50000);
    console.log("✅ Dépense allouée correctement au projet");
  });
  it("calcule correctement la progression du projet", function () {
    var projet = creerProjet("Formation", 100000);
    projet = allouerDepense(projet, 25000);
    var progression = calculerProgression(projet);
    expect(progression).toBe(25);
    console.log("✅ Progression du projet calculée correctement");
  });
  it("simule l'affichage des projets et de leur progression", function () {
    var DummyComponent = function DummyComponent() {
      return (0, _jsxRuntime.jsxs)(_jsxRuntime.Fragment, {
        children: [(0, _jsxRuntime.jsx)(_reactNative.Text, {
          children: "Projet: D\xE9m\xE9nagement - Progression: 0%"
        }), (0, _jsxRuntime.jsx)(_reactNative.Text, {
          children: "Projet: Voyage - Progression: 25%"
        })]
      });
    };
    var _render = (0, _reactNative2.render)((0, _jsxRuntime.jsx)(DummyComponent, {})),
      getByText = _render.getByText;
    expect(getByText("Projet: Déménagement - Progression: 0%")).toBeTruthy();
    expect(getByText("Projet: Voyage - Progression: 25%")).toBeTruthy();
    console.log("✅ Composants des projets et de leur progression affichés correctement");
  });
});
